{
  "name": "@svifty7/eslint-config",
  "type": "module",
  "version": "0.0.2",
  "packageManager": "pnpm@10.10.0",
  "description": "ESLint config by svifty7 based on @antfu/eslint-config",
  "author": "svifty7 (https://github.com/svifty7)",
  "license": "MIT",
  "homepage": "https://github.com/svifty7/eslint-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/svifty7/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/svifty7/eslint-config/issues"
  },
  "keywords": [
    "eslint-config"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "nlx @eslint/config-inspector --config eslint.config.ts",
    "build": "nr gen && tsdown",
    "build:inspector": "pnpm build && nlx @eslint/config-inspector build",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "gen": "tsx scripts/typegen.ts",
    "prepack": "nr build",
    "release": "bumpp && pnpm publish",
    "prepare": "simple-git-hooks"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "eslint": "^9.26.0",
    "vue": "^3.5.14"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    }
  },
  "dependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint/markdown": "^6.4.0",
    "@prettier/plugin-xml": "^3.4.1",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitest/eslint-plugin": "^1.1.44",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-flat-config-utils": "^2.0.1",
    "eslint-merge-processors": "^2.0.0",
    "eslint-plugin-antfu": "^3.1.1",
    "eslint-plugin-command": "^3.2.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-import-x": "^4.11.1",
    "eslint-plugin-jsdoc": "^50.6.15",
    "eslint-plugin-jsonc": "^2.20.0",
    "eslint-plugin-n": "^17.18.0",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-perfectionist": "^4.13.0",
    "eslint-plugin-pnpm": "^0.3.1",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-toml": "^0.12.0",
    "eslint-plugin-unicorn": "^59.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^10.1.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "eslint-plugin-yml": "^1.18.0",
    "globals": "^16.1.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^1.1.1",
    "pnpm-workspace-yaml": "^0.3.1",
    "prettier": "^3.5.3",
    "toml-eslint-parser": "^0.10.0",
    "vue-eslint-parser": "^10.1.3",
    "yaml-eslint-parser": "^1.3.0"
  },
  "devDependencies": {
    "@antfu/ni": "^24.3.0",
    "@eslint/config-inspector": "^1.0.2",
    "@types/node": "^22.14.0",
    "bumpp": "^10.1.0",
    "eslint": "^9.26.0",
    "eslint-typegen": "^2.2.0",
    "jiti": "^2.4.2",
    "lint-staged": "^16.0.0",
    "simple-git-hooks": "^2.13.0",
    "tinyglobby": "^0.2.13",
    "tsdown": "^0.11.9",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vue": "^3.5.14"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
